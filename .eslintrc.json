{
    "extends": ["node/style-guide"],
    "plugins": [
      "import"
    ],
    "rules": {
        "object-curly-newline": ["error", { "multiline": true }],
        "linebreak-style": 0,
        "max-len": ["error", { "code": 238 }],
        "no-underscore-dangle": 0,
        "no-console": 0,
        "import/extensions": 0,
        "consistent-this": 0
      },
      "env": {
        "browser": true,
        "es6": true
      }
}
