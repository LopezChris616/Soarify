{
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "react/prop-types": [0],
        "object-curly-newline": ["error", "never"],
        "linebreak-style": 0,
        "max-len": ["error", { "code": 238 }],
        "no-underscore-dangle": 0,
        "react/jsx-one-expression-per-line": 0,
        "no-console": 0
      },
      "parser": "babel-eslint",
      "env": {
        "browser": true
      }
}
